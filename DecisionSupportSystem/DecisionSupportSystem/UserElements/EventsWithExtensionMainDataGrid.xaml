<UserControl x:Class="DecisionSupportSystem.UserElements.EventsWithExtensionMainDataGrid"
                         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:task="clr-namespace:DecisionSupportSystem.UserElements"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Name="MainGrid">
        <task:EventsWithExtensionDataGrid DataContext="{Binding ElementName=MainGrid, Path=DataContext}" Margin="0,0,0,25"/>
        <task:ProbabilitySumUE DataContext="{Binding Path=ProbabilitySumViewModel}" VerticalAlignment="Bottom" Height="25"/>
    </Grid>
</UserControl>
